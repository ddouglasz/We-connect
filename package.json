{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "proxy": "http://localhost:8000",
  "scripts": {
    "test:dev": "NODE_ENV=tests nyc mocha ./server/test/ --compilers js:babel-core/register --exit",
    "build": "babel server -d server/dist",
    "un-migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "db-migrate": "npm run un-migrate && node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "start": "babel-node app.js",
    "start:dev": "nodemon --watch server --exec  babel-node app.js",
    "lint": "eslint ./ || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --timeout 7000 --compilers js:babel-register server/test || true",
    "postinstall": "npm run build",
    "heroku-postbuild": "npm run db-migrate",
    "webpack": "webpack-dev-server -d --config ./webpack.config.dev.js --hot"
  },
