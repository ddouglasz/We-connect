    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.36.1",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.0.0",
    "validator": "^9.4.1",
    "webpack-preset": "^0.2.0",
    "webpack-preset-babel": "^0.2.0",
    "webpack-preset-babel-stage-2": "^0.2.0"
