    "dotenv": "^5.0.1",
    "eslint": "^4.9.0",
    "eslint-config-react": "^1.1.7",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.9.0",
