    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.9.0",
    "nyc": "^11.4.1",
